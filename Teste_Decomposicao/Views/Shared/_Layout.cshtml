<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Teste Decomposicao</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Teste_Decomposicao.styles.css" asp-append-version="true" />
    <link href="~/DataTable/datatables.min.css" rel="stylesheet" />
</head>
<body>
    <div class="container">
        <ul class="nav">
            <li class="nav-item">
                <a class="color" class="nav-link" asp-area="" asp-controller="Home" asp-action="Home">Home</a>
            </li>
            <li class="nav-item">
                <a class="color" class="nav-link active" aria-current="page" asp-area="" asp-controller="Home" asp-action="Index">Currículo</a>
            </li>
            <li class="nav-item">
                <a class="color" class="nav-link" asp-area="" asp-controller="Home" asp-action="DecomposicaoValores">Teste</a>
             </li>
            <li class="nav-item">
                <a class="color" class="nav-link" asp-area="" asp-controller="Home" asp-action="SobreMim">Sobre mim</a>
            </li>
         </ul>
    </div>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/DataTable/datatables.min.js"></script>
    
    
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>